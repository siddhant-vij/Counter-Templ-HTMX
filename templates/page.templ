package templates

templ Page(global, session int) {
	<!DOCTYPE html>
	<html lang="en">
		<head>
			<meta charset="UTF-8"/>
			<meta name="viewport" content="width=device-width, initial-scale=1.0"/>
			<link rel="stylesheet" href="/static/reset.css"/>
			<link rel="stylesheet" href="/static/styles.css"/>
			<title>Counter App</title>
		</head>
		<body>
			@Header()
			<main>
				@Global(global)
				@Session(session)
			</main>
		</body>
	</html>
}
